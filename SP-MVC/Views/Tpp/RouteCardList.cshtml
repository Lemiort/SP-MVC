@model IEnumerable<SP_MVC.RouteCar>

@{
    ViewBag.Title = "RouteCardList";
}

<h2>RouteCardList</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Route.NameTechProc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompanyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Developer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Checked)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Agreed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Approved)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NormСontroller)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Route.NameTechProc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompanyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Developer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Checked)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Agreed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Approved)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NormСontroller)
        </td>
        <td>
            @Html.ActionLink("Delete", "DeleteRouteCard", new { id=item.RouteCarId })
        </td>
    </tr>
}

</table>
